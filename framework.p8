pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
--main
function _init()
 map_setup()
 make_player()
end

function _update()
 player_update()
 camera_update()
end

function _draw()
 cls()
 draw_map()
 draw_player()
end
-->8
--map

function map_setup()
	solid = 0
	key = 1
	door = 2
	anim1 = 3
	anim2 = 4
	lose = 6
	win = 7
	--tile width
	tilew = 16
	--map width and height
	mapw = 128 * tilew
	maph = 64 * tilew
end

function draw_map()
	map(0,0,0,0,128,64)
end

function is_tile(x,y,tile_type)
 local x = flr(x/8)
 local y = flr(y/8)
 tile=mget(x,y)
 has_flag=fget(tile, tile_type)
 return has_flag
end

function can_move(x,y)
 if (is_tile(x,y,solid) or
 is_tile(x+7,y,solid) or
 is_tile(x+7,y+7,solid)
 or is_tile(x,y+7,solid)) then
  return false
 else
  return true
 end
 
end
-->8
--player

function make_player()
 p={
  x = 0,
  y = 8,
  speed = 1,
  timer=0,
  xinput = 0,
  yinput = 0,
  
  sprite=16,
  sprited=16,
  spriteu=17,
  spritel=18,
  spriter=19
 }
end

function player_update_inputs()
	p.xinput = 0
	p.yinput = 0
 if (btn(⬅️)) then 
  p.xinput-=1
 elseif (btn(➡️)) then
  p.xinput+=1
 end
 if (btn(⬆️)) then
  p.yinput-=1
 elseif (btn(⬇️)) then
  p.yinput+=1
 end
end

function player_move()
 local xvel = p.xinput * p.speed
 local yvel = p.yinput * p.speed
 local newx = p.x + xvel
 local newy = p.y + yvel
 
 if (can_move(newx, p.y)) then
  local maxx=mapw-tilew
  p.x = mid(0, newx, maxx)
 end
 
 if (can_move(p.x, newy)) then
  local maxy=maph-tilew
  p.y = mid(0, newy, maxy)
 end
 
 --footsteps
 if (p.xinput!=0 or p.yinput!=0) then
  p.timer+=1
  p.timer%=10
  if (p.timer == 0) sfx(0)
 end
end

function player_update_sprite()
 if p.xinput == 1 then
  p.sprite = p.spriter
 elseif p.xinput == -1 then
  p.sprite = p.spritel
 elseif p.yinput == 1 then
  p.sprite = p.sprited
 elseif p.yinput == -1 then
  p.sprite = p.spriteu
 end
end

function player_update()
 player_update_inputs()
 player_move()
 player_update_sprite()
end

function draw_player()
 spr(p.sprite, p.x, p.y)
end
-->8
--camera

function camera_update()
 local x = flr(p.x) - 64 + 4
 local y = flr(p.y) - 64 + 4
 x = max(0,x)
 y = max(0,y)
 camera(x,y)
end
-->8
--entities
entities = {}

for i = 1,10 do
 entities[i] = -1
end

function entities:getcount()
 local count = 0
 for i = 1,10 do
	 if (entities[i] != -1) count+=1
	end
	return count
end

function entities:new()
 local e = {
  x = 0,
  y = 0,
  ydraw = 0,
  state = 0,
  sprite = 1,
  health = 3
 }
 
 return e
end

function entities:update()
 for e in pairs(self) do
  e:update()
 end
end

function entities:draw()
 for e in pairs(self) do
  e:draw()
 end
end
